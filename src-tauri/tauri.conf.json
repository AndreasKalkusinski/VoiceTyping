{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Voice Typing",
  "version": "0.1.7",
  "identifier": "com.voicetyping.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/AndreasKalkusinski/VoiceTyping/releases/latest/download/latest.json"
      ],
      "pubkey": ""
    }
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Voice Typing",
        "width": 420,
        "height": 520,
        "resizable": true,
        "decorations": false,
        "transparent": true,
        "center": true,
        "minWidth": 320,
        "minHeight": 400
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": "3252C3BF6642EA3019D5FCA8D15776919B0F5A50",
      "timestampUrl": "http://timestamp.digicert.com"
    },
    "macOS": {
      "signingIdentity": "Developer ID Application: Andreas Kalkusinski (9RN6UD7PMA)",
      "entitlements": "entitlements.plist",
      "infoPlist": "Info.plist",
      "minimumSystemVersion": "10.15"
    },
    "publisher": "JeanTools"
  }
}
